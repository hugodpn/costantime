<div id="sidebar">
  <ul>
    <li>
      <h1>Listing users</h1>
    </li>
  </ul>
</div>

<div id="contentPage">
  <table id="rounded-corner">
    <tr>      
      <th class="rounded-top-left">Login</th>
      <th>Email</th>
      <th colspan="2" class="rounded-top-right"></th>
    </tr>

    <% @users.each do |user| %>
      <tr>
        <td><%=h user.login %></td>
        <td><%=h user.email %></td>
        <td><%= link_to 'Edit', edit_user_path(user) %></td>
        <td><%= link_to 'Destroy', user, :confirm => 'Are you sure?', :method => :delete %></td>
      </tr>
    <% end %>
  </table>

  <br />

  <%= link_to 'New user', new_user_path %>
</div>