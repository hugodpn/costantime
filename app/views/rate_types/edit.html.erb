<div id="contentPage">
  <h1>Editing rate_type</h1>

  <% form_for(@rate_type) do |f| %>
    <%= f.error_messages %>

    <p>
      <%= f.label :name %><br />
      <%= f.text_field :name %>
    </p>
    <p>
      <%= f.submit 'Update' %>
    </p>
  <% end %>

  <%= link_to 'Show', @rate_type %> |
  <%= link_to 'Back', rate_types_path %>
</div>